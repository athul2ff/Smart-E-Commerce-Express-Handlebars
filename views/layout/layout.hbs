<style>
    @import url(https://fonts.googleapis.com/css?family=Raleway:400,500);


headers {
  font-size: 2em;
  font-weight: bold;
  margin: 20px;
}

#form-outer {
  background-color: rgba(218, 61, 61, 0.87);
  margin: 0 auto;
  border-radius: 4px;
  width: 75%;
  max-width: 900px;
  padding: 10px;
  padding-top: 20px;
}
#boc{
     text-align: center;
  font-family: 'Raleway', Helvetica, sans-serif;
  min-width: 320px;
}

.labeled {
  display: inline-block;
  text-align: right;
  width: 40%;
  padding: 5px;
  vertical-align: top;
  margin-top: 10px;
}

.rightelement {
  display: inline-block;
  text-align: left;
  width: 48%;
  vertical-align: middle;
}

.inputs {
  height: 30px;
  width: 280px;
  padding: 5px;
  margin: 10px;
  border: 1px solid #c0c0c0;
  border-radius: 2px;
}

#userAge {
  width: 40px;
}

.rating,
input[type="checkbox"] {
  float: left;
  margin-right: 5px;
}

#submit {
  background-color: #59ace0;
  border-radius: 4px;
  color: white;
  font-size: 1em;
  height: 40px;
  width: 96px;
  margin: 10px;
  border: 0px solid;
}

.dropdown {
  height: 35px;
  width: 140px;
  padding: 5px;
  margin: 10px;  
  margin-top: 15px;
  border: 1px solid #c0c0c0;
  border-radius: 2px;
}

.radio, .checkbox {
  position: relative;
  left: -43px;
  margin-left: 10px;
  display: block;
  padding-bottom: 10px;
}

@media screen and (max-width: 833px) {
  .inputs {
    width: 80%;
  }
  select {
    width: 90%;
  }
}

@media screen and (max-width: 520px) {
  .labeled {
    width: 100%;
    text-align: left;
  }
  .rightelement {
    width: 80%;
    float: left;
  }
  .inputs {
    width: 100%;
  }
  select {
    width: 100%;
  }
}
</style>



</body>

